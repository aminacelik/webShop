$('#cart').html("<%= escape_javascript render(@cart) %>");

if ($('#cart_container') != null) {
	$('#cart_container').html("<%= escape_javascript render partial: 'carts/detailed_cart', locals: {items: @cart.line_items } %>");
}
else {
	$('#product_list').html("<%= escape_javascript render partial: 'carts/detailed_cart', locals: {items: @cart.line_items } %>");

}
