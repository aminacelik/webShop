+<%= stylesheet_link_tag "homepage" %>


	<% cache ['store', Product.latest] do %>

	<h2><%= t('.new_in_store') %></h2>
		<% @products.each do |product| %>

			<% cache ['entry', product] do %>
				<%= render partial: "products/product", locals: {product: product} %>
			<% end %>

		<% end %>
	<% end %>
